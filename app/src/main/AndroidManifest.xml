<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="app.daytoday.softpro.daytodayindiadtd">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="app.daytoday.softpro.daytodayindiadtd.SplashScreen"
            android:label="          Day Today India"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="app.daytoday.softpro.daytodayindiadtd.WelcomeActivity"
            android:label="          Day Today India"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="app.daytoday.softpro.daytodayindiadtd.AdminLogin" />
        <activity android:name="app.daytoday.softpro.daytodayindiadtd.SportsActivity" />
        <activity android:name="app.daytoday.softpro.daytodayindiadtd.EntertainmentActivity" />
        <activity android:name="app.daytoday.softpro.daytodayindiadtd.AdminDashboard" />
        <activity android:name="app.daytoday.softpro.daytodayindiadtd.AddPost" />
        <activity android:name="app.daytoday.softpro.daytodayindiadtd.FullscreenNews" />
        <activity android:name="app.daytoday.softpro.daytodayindiadtd.DeleteDashboard" />
        <activity android:name="app.daytoday.softpro.daytodayindiadtd.Deletelist" />
        <activity android:name="app.daytoday.softpro.daytodayindiadtd.DestroyActivity" />

        <activity android:name="app.daytoday.softpro.daytodayindiadtd.FavouriteList" />
        <activity android:name="app.daytoday.softpro.daytodayindiadtd.AddFavActivity"></activity>
        <!--fcm-->
        <service android:name="app.daytoday.softpro.daytodayindiadtd.MyFirebaseMessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>

        <service android:name="app.daytoday.softpro.daytodayindiadtd.MyFirebaseInstanceIdService">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT" />
            </intent-filter>
        </service>
    </application>

</manifest>